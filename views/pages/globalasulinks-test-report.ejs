<% include ../partials/header %>

    <div class="container">

      <h1>Global ASU Links test report.</h1>

      <hr />

      <h4>Report ID: <a href="/report/<%= data.siteID %>"><%= data.siteID %></a></h4>
      <h4>Page ID: <%= data._id %></h4>
      <h4>Page Link: <a href="<%= data.pageLink %>" target="_blank"><%= data.pageLink %></a></h4>
      <h4>Page Title: <%= data.results[0].pageTitle %></h4>
      <h2>Grade: <span <% if (globalasulinksgrade <= 70) { %>
        class="bg-danger text-white"<% }
        else if (globalasulinksgrade <= 80) { %>
         class="bg-warning text-white"<% }
       else { %>
         class="bg-success text-white"<%
       }%>><%= globalasulinksgrade %></span></h2>

      <hr />

      <p>Global ASU Links on page: <%= data.results[0].globalasulinks %></p>

      <p>Global ASU Links expected: <%= baselineLinksObject %></p>

    </div>

<% include ../partials/footer %>
