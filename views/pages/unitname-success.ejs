<% include ../partials/head %>

    <div class="container">

      <h1><%= title %> Results</h1>

      <h3>You are testing page: <a href="<%= page %>" target="_blank"><%= page %></a></h3>

      <div class="row">
        <div class="col-md-12">
        <table class="table table-hover">
          <caption>Unit Names on page: <%= pf.length %></caption>
          <thead>
            <tr>
              <th>Text</th>
              <th>Casing Type</th>
              <th>Pass/Fail</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i=0; i < pf.length; i++) { %>
               <tr>
                 <td><%= pf[i].text %></td>
                 <td><%= pf[i].casing %></td>
                 <td><%= pf[i].passfail %></td>
               </tr>
            <% } %>
          </tbody>
        </table>
      </div>
      </div>

    </div>

<% include ../partials/footer %>
