<% include ../partials/testrunner-head %>

    <div class="container">

      <h1>Page report.</h1>
      <p><a href="/testrunner/savesitereport/<%= reportid %>/<%= pageid %>">Save pdf.</a></p>

      <hr />

      <h4>Report ID: <a href="/testrunner/report/<%= reportid %>"><%= reportid %></a></h4>
      <h4>Page ID: <%= pageid %></h4>
      <h4>Page link: <a href="<%= buttonstests[0].pageLink %>" target="_blank"><%= buttonstests[0].pageLink %></a></h4>
      <h4>Page Title: <%= buttonstests[0].results[0].pageTitle %></h4>
      <h2>Page Grade: Grade <%= Math.round(((unitnamegrade+globalasulinksgrade+buttonsgrade)/300)*100) %></h2>

      <hr />

      <h4><a href="/testrunner/report/<%= reportid %>/<%= pageid %>/unitname">Unit Name Grade</a>: <%= unitnamegrade %></h4>

      <h4><a href="/testrunner/report/<%= reportid %>/<%= pageid %>/globalasulinks">Global ASU Links Grade</a>: <%= globalasulinksgrade %></h4>

      <h4><a href="/testrunner/report/<%= reportid %>/<%= pageid %>/buttons">Buttons Grade</a>: <%= buttonsgrade %></h4>

    </div>

<% include ../partials/testrunner-footer %>
