<% include ../partials/head %>

    <div class="container">

      <h1><%= title %> Results</h1>

      <h3>You are testing page: <%= page %></h3>

      <div class="row">
        <div class="col-md-12">
        <table class="table table-hover">
          <caption>Total links: <%= results.length %></caption>
          <thead>
            <tr>
              <th>Link</th>
            </tr>
          </thead>
          <tbody>
            <% for(var i=0; i < results.length; i++) { %>
               <tr>
                 <td><%= results[i].link %></td>
               </tr>
            <% } %>
          </tbody>
        </table>
      </div>
      </div>

    </div>

<% include ../partials/footer %>
